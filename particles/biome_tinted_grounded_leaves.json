{
	"format_version": "1.10.0",
	"particle_effect": {
		"description": {
			"identifier": "ii:grounded_leaves",
			"basic_render_parameters": {
				"material": "particles_alpha",
				"texture": "textures/particle/biome_tinted_leaves_atlas"
			}
		},
		"components": {
			"minecraft:emitter_rate_manual": {
				"max_particles": 500
			},
			"minecraft:emitter_lifetime_expression": {
				"activation_expression": 1,
				"expiration_expression": 0
			},
			"minecraft:emitter_shape_point": {},
			"minecraft:particle_initial_spin": {
				"rotation": "math.random_integer(0, 359)"
			},
			"minecraft:particle_lifetime_expression": {
				"max_lifetime": "Math.random(30, 90)"
			},
			"minecraft:particle_appearance_billboard": {
				"size": [
					"v.particle_random_1 > 0.5 ? 0.1 : 0.15",
					"v.particle_random_1 > 0.5 ? 0.1 : 0.15"
				],
				"facing_camera_mode": "emitter_transform_xz",
				"uv": {
					"texture_width": 28,
					"texture_height": 21,
					"uv": [
							"math.floor(v.particle_random_1 * 4) * 7",
							"math.floor(v.particle_random_2 * 3) * 7"
					],
					"uv_size": [ 7, 7 ]
				}
			},
			"minecraft:particle_appearance_lighting": {},
			"minecraft:particle_appearance_tinting": {
				"color": {
					"interpolant": "v.particle_age / v.particle_lifetime",
					"gradient": {
						"0.0": "#FF5BB81D",
						"0.48": "#FFF4BC33",
						"1.0": "#FFA97358"
					}
				}
			}
		}
	}
}
