			"loop" : true,
			"bones" : {
				"head" : {
					"rotation" : [ "math.max(query.target_x_rotation - 60, -100)", "query.target_y_rotation", "-query.target_y_rotation" ]
				}
			}
		},
		"animation.humanoid.look_at_target.swimming" : {
			"loop" : true,
			"bones" : {
				"head" : {
					"rotation" : [ "math.max(query.target_x_rotation - 60, -100)", "query.target_y_rotation", "-query.target_y_rotation" ]
				}
