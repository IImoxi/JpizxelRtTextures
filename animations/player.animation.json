					"position" : [ 0.0, "((math.cos(query.life_time * 103.2) * 0.12) + 0.06)", 0.0 ]
				}
			}
		},
		"animation.player.move.legs" : {
			"loop": true,
			"bones": {
				"leftleg": {
					"rotation": [ "variable.tcos0 * -1.4", 0.1, 0.1 ]
				},
				"rightleg": {
					"rotation": [ "variable.tcos0 * 1.4", 0.1, 0.1 ]
				},
				"root": {
					"position": [ 0, "-(math.cos(q.modified_distance_moved * 76.34) * ((1.15 - q.modified_move_speed) * q.modified_move_speed)) * 2", 0]
