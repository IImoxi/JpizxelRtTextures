{
  "namespace": "hud",

	// Chat
	
	"chat_text": {
		"type": "label",
		"layer": 10,
		"text": "#text",
		"size": [ 268, "default" ],
		"shadow": true,
		"enable_profanity_filter": false,
		"localize": true,
		"font_type": "$chat_font_type",
		"font_scale_factor": 0.6666,
		"color": "$chat_text_color",
		"anims": [
		"@hud.ii_slide_anim",
		"@hud.ii_fade_anim"
		]
	},
	
	"single_chat_panel@start.background": {
		"layer": 1,
		"size": [ "100%", "100%c" ],
		"anims": [ "@hud.ii_bg_anim" ],
		"controls": [
		{
			"single_chat_label@hud.chat_text": {}
		}
		]
	},
	
	"ii_chat_panel": {
		"type": "panel",
		"clips_children": true,
		"anchor_from": "bottom_left",
		"anchor_to": "bottom_left",
		"size": [ 270, "30%" ],
		"offset": [ 1, -31 ],
		"controls": [
			{
				"stack_panel": {
					"type": "stack_panel",
					"anchor_from": "bottom_left",
					"anchor_to": "bottom_left",
					"factory": {
						"name": "chat_item_factory",
						"max_children_size": 10,
						"control_ids": {
							"chat_item": "single_chat_panel@hud.single_chat_panel"
						}
					}
				}
			}
		],
		"bindings": [
			{
				"binding_name": "#hud_visible",
				"binding_name_override": "#visible",
				"binding_type": "global"
			}
		]
	},

	// Animations - Chat
	
	"ii_slide_anim": {
		"anim_type": "offset",
		"easing": "out_quint",
		"duration": 0.4,
		"from": [ -8, 0 ],
		"to": [ 0, 0 ],
		"next": "@hud.ii_slide_anim_wait"
	},
	
	"ii_slide_anim_wait": {
		"anim_type": "wait", "duration": "$chat_item_lifetime", "next": "@hud.ii_slide_anim_out"
	},
	
	"ii_slide_anim_out": {
		"anim_type": "offset",
		"easing": "in_quint",
		"duration": 0.5,
		"from": [ 0, 0 ],
		"to": [ -8, 0 ]
	},
	
	"ii_fade_anim": {
		"anim_type": "alpha", "easing": "out_cubic", "duration": 0.4, "from": 0.5, "to": 1, "next": "@hud.ii_fade_anim_wait"
	},
	
	"ii_fade_anim_wait": {
		"anim_type": "wait", "duration": "$chat_item_lifetime", "next": "@hud.ii_fade_anim_out"
	},
	
	"ii_fade_anim_out": {
		"anim_type": "alpha", "easing": "in_cubic", "duration": 0.5, "from": 1, "to": 0
	},

	"ii_bg_anim": {
		"anim_type": "alpha", "easing": "out_quint", "duration": 0.4, "from": 0, "to": 0.4, "next": "@hud.ii_bg_anim_wait"
	},
	
	"ii_bg_anim_wait": {
		"anim_type": "wait", "duration": "$chat_item_lifetime", "next": "@hud.ii_bg_anim_out"
	},
	
	"ii_bg_anim_out": {
		"anim_type": "alpha", "easing": "in_cubic", "duration": 0.5, "from": 0.4, "to": 0, "destroy_at_end": "single_chat_panel"
	},
	
	// Custom Hotbar
	
	"hotbar": {
		"type": "image",
		"size": [ 182, 22 ],
		"texture": "textures/gui/hotbar",
		"bindings": [
			{
				"binding_name": "#hud_visible",
				"binding_name_override": "",
				"binding_type": "global"
			}
		]
	},

	
	// Player Pos
	
	"player_position@start.background": {
    "size": [ "100%c + 4px", "100%c" ],
	"offset": [ 1, 1 ],
    "controls": [
      {
        "player_position_text": {
          "type": "label",
          "anchor_from": "bottom_middle",
          "anchor_to": "bottom_middle",
          "layer": 1,

          "enable_profanity_filter": false,
          "color": "$chat_text_color",
          "text": "#text",
          "shadow": true,

          "bindings": [
            {
              "binding_name": "#player_position_text",
              "binding_name_override": "#text",
              "binding_condition": "always_when_visible",
              "binding_type": "global"
            },
			{
				"binding_name": "#hud_visible",
				"binding_name_override": "#visible",
				"binding_type": "global"
			}
          ]
        }
      }
    ],
    "bindings": [
      {
        "binding_name": "#player_position_visible",
        "binding_name_override": "#visible",
        "binding_condition": "always",
        "binding_type": "global"
      }
    ]
  },
	
	// Rest
	
	//Actionbar
	
	"hud_actionbar_text@start.background": {
		"size": [ "100%c + 8px", "100%c + 3px" ],
		"offset": [ 0, "50%-64px" ],
		"alpha": "@hud.anim_actionbar_text_background_alpha_out",
		"controls": [
		{
			"actionbar_message": {
				"type": "label",
				"anchor_from": "center",
				"anchor_to": "center",
				"color": "$tool_tip_text",
				"layer": 1,
				"text": "$actionbar_text",
				"localize": false,
				"alpha": "@hud.anim_actionbar_text_alpha_out"
			}
		}
		],
		"bindings": [
			{
				"binding_name": "#hud_visible",
				"binding_name_override": "#visible",
				"binding_type": "global"
			}
		]
	},
	
	"anim_actionbar_text_background_alpha_out": {
		"anim_type": "alpha",
		"easing": "in_expo",
		"duration": 2,
		"from": 0.4,
		"to": 0,
		"destroy_at_end": "hud_actionbar_text"
	},
	
	"anim_actionbar_text_alpha_out": {
		"anim_type": "alpha",
		"easing": "in_expo",
		"duration": 2,
		"from": 1.0,
		"to": 0,
		"destroy_at_end": "hud_actionbar_text"
    },
	
	// Hotbar fix
	
	"hotbar_slot_image": {
		"type": "image",
		"offset": [ 0, 0 ],
		"size": [ 20, 22 ],
		"layer": 1,
		"alpha": 1
	},
	
	"start_cap_image": {
		"type": "image",
		"texture": "textures/ui/hotbar_start_cap",
		"anchor_from": "top_left",
		"anchor_to": "top_left",
		"size": [ 1, 22 ],
		"layer": 4,
		"alpha": 1,
		"visible": true
	},
	
	"gui_hotbar_grid_item": {
		"type": "panel",
		"layer": 1,
		"size": [ 20, 22 ],
		"anchor_to": "top_left",
		"anchor_from": "top_left",
		"$stack_count_required|default": true,
		"$durability_bar_required|default": true,
		"controls": [
			{ "hotbar_slot_selected_image@hotbar_slot_selected_image": {} },
			{ "hotbar_renderer@hotbar_renderer": {} },
			{
				"hotbar_parent": {
					"type": "panel",
					"size": [ 18, 18 ],
					"controls": [
						{
							"item_icon@hotbar_hud_item_icon": {
								"layer": 9,
								"size": [ 16, 16 ]
							}
						},
						{
							"item_count@common.stack_count_label": {
								"layer": 11,
								"$item_collection_name": "$hotbar_collection_name",
								"$stack_count_collection_name": "$hotbar_collection_name",
								"bindings": [
									{
										"binding_name": "#inventory_stack_count",
										"binding_type": "collection",
										"binding_collection_name": "$stack_count_collection_name"
									},
									{
										"binding_name": "#stack_count_visible",
										"binding_name_override": "#visible",
										"binding_type": "collection",
										"binding_collection_name": "$stack_count_collection_name"
									}
								]
							}
						},
						{
							"item_dura@item_durability": {
								"layer": 10
							}
						},
						{
						  "container_item_lock_overlay@hud.container_item_lock_overlay": {
								"layer": 12,
								"size": [ 16, 16 ],
								"bindings": [
									{
										"binding_name": "#hud_visible",
										"binding_name_override": "#visible",
										"binding_type": "global"
									}
								]
							}
						}
					]
				}
			},
			{ 
				"cooldown_renderer@cooldown_renderer":  { 
					"layer": 10,
					"bindings": [
						{
							"binding_name": "#hud_visible",
							"binding_name_override": "#visible",
							"binding_type": "global"
						}
					]
				} 
			},
			{ 
				"hotbar_slot_button@gui_hotbar_slot_button_prototype": {
					"bindings": [
						{
							"binding_name": "#hud_visible",
							"binding_name_override": "#visible",
							"binding_type": "global"
						}
					]
				} 
			}
		],
		"bindings": [ 
			{}
		]
	},

	// Anims
	
	"anim_item_name_text_alpha_in": {
		"duration": 0.08,
		"from": 0,
		"to": 1.0,
		"next": "@hud.anim_item_name_text_alpha_stay"
	},
	
	"anim_item_name_text_alpha_stay": {
		"duration": 0
	},
	
	"anim_item_name_text_alpha_out": {
		"duration": 0.55,
		"from": 1.0,
		"to": 0
	},
  
	"anim_item_name_background_alpha_in": {
		"duration": 0.08,
		"from": 0,
		"to": 0.4
	},
	
	"anim_item_name_background_alpha_stay": {
		"duration": 0
	},
	
  "anim_item_name_background_alpha_out": {
		"duration": 0.5,
		"from": 0.4,
		"to": 0
	},
  
	"vignette_renderer": {
		"type": "custom",
		"renderer": "",
		"layer": 5
	},
  

    "bottom_hud": {
		"type": "panel",
		"anchor_from": "bottom_middle",
		"anchor_to": "bottom_middle",
		"size": [ 180, 50 ],
		"controls": [
			{
				"heart_rend@heart_renderer": {
					"offset": [ -1, -28 ],
					"anchor_from": "bottom_left",
					"anchor_to": "bottom_left"
				}
			},
			{
				"horse_heart_rend_0@horse_heart_renderer": {
					"offset": [ 180, -44 ],
					"anchor_from": "bottom_left",
					"anchor_to": "bottom_left",
					"bindings": [
						{
							"binding_name": "#is_creative_mode",
							"binding_name_override": "#visible"
						}
					]
				}
			},
			{
				"horse_heart_rend_1@horse_heart_renderer": {
					"offset": [ 180, -38 ],
					"anchor_from": "bottom_left",
					"anchor_to": "bottom_left",
					"bindings": [
						{
							"binding_name": "#show_survival_ui",
							"binding_name_override": "#visible"
						}
					]
				}
			},
			{
				"armor_rend@armor_renderer": {
					"offset": [ -1, -28 ],
					"anchor_from": "bottom_left",
					"anchor_to": "bottom_left",
					"bindings": [
						{
							"binding_name": "not #is_creative_mode",
							"binding_name_override": "#visible"
						},
						{
							"binding_name": "#hud_visible",
							"binding_name_override": "#visible",
							"binding_type": "global"
						}
					]
				}
			},
			{
				"hunger_rend@hunger_renderer": {
					"offset": [ 180, -28 ],
					"anchor_from": "bottom_left",
					"anchor_to": "bottom_left"
				}
			},
			{
				"bubbles_rend_0@bubbles_renderer": {
					"offset": [ 180, -38 ],
					"anchor_from": "bottom_left",
					"anchor_to": "bottom_left",
					"bindings": [
						{
							"binding_name": "#is_not_riding",
							"binding_name_override": "#visible"
						}
					]
				}
			},
			{
				"bubbles_rend_1@bubbles_renderer": {
					"offset": [ 180, -58 ],
					"anchor_from": "bottom_left",
					"anchor_to": "bottom_left",
					"bindings": [
						{
							"binding_name": "#is_riding",
							"binding_name_override": "#visible"
						}
					]
				}
			},
			{ "exp_rend@exp_progress_bar_and_hotbar": { "bindings": [ {} ]} } // for classic
		],
		"bindings": [ { } ]
	},
  
  // ROOTs PANEL

	"root_panel": {
		"type": "panel",
		"$xp_control_offset|default": [ 0, -13 ],
		"variables": [
			{
				"requires": "(not $desktop_screen)",
				"$xp_control_offset": [ 0, -11 ]
			},
			{
				"requires": "$education_edition",
				"$left_helpers": "hud.left_helpers_edu"
			},
			{
				"requires": "(not $education_edition)",
				"$left_helpers": "hud.left_helpers"
			}
		],
		"controls": [
			{ "chat@hud.ii_chat_panel": {} },
			{ "player_position@hud.player_position": { "anchor_from": "top_left", "anchor_to": "top_left" } },
			{ "curor_rend@cursor_renderer": {} },
			{ "sidebar@scoreboard.scoreboard_sidebar": {} },
			{ "hud_tip_text_factory": { "type": "factory", "control_name": "@hud.hud_tip_text" } },
			{ "mob_effects_renderer@mob_effects_renderer": { 
				"size": [ "100%", "100%" ], "offset": [ 0, 24 ], "anchor_from": "center", "anchor_to": "center",
				"bindings": [
						{
							"binding_name": "#hud_visible",
							"binding_name_override": "#visible",
							"binding_type": "global"
						}
					]
				} 
			},
			{
				"hud_actionbar_text_area": {
					"type": "panel",
					"factory": {
						"name": "hud_actionbar_text_factory",
						"control_ids": { "hud_actionbar_text": "hud_actionbar_text@hud.hud_actionbar_text" }
					},
					"bindings": [
						{
							"binding_name": "#hud_visible",
							"binding_name_override": "#visible",
							"binding_type": "global"
						}
					]
				}
			},
			{
				"hud_title_text_area": {
					"type": "panel",
					"factory": {
						"name": "hud_title_text_factory",
						"control_ids": { "hud_title_text": "hud_title_text@hud.hud_title_text" }
					},
					"bindings": [
						{
							"binding_name": "#hud_visible",
							"binding_name_override": "#visible",
							"binding_type": "global"
						}
					]
				}
			},
			{
				"boss_health_panel": {
					"type": "stack_panel", "orientation": "vertical",
					"size": [ "100%", "100%" ],
					"controls": [
						{
							"boss_hud_padding": {
								"type": "panel",  "size": [ "100%", 20 ],
								"visible": "#boss_hud_padding",
								"bindings": [ { "binding_name": "#boss_hud_padding", "binding_name_override": "#visible" } ]
							}
						},
						{
							"boss_hud_panel": {
								"type": "panel",
								"controls": [
									{
										"boss_health_grid@boss_health_grid": {
											"offset": [ 0, 2 ]
										}
									}
								]
							}
						}
					],
					"bindings": [
						{
							"binding_name": "#hud_visible",
							"binding_name_override": "#visible",
							"binding_type": "global"
						}
					]
				}
			}
		],
		"bindings": [
			{
			}
		]
	},
	
	"hud_content": {
		"type": "panel",
		"$hud_size": [ "100%", "100%" ],
		"$hud_player_rend_desktop_offset|default": [ 15, 15 ],
		"variables": [
			{
				"requires": "($is_holographic and not $is_living_room_mode)",
				"$hud_player_rend_desktop_offset": [ 100, 50 ]
			},
			{
				"requires": "($compress_hud_width)",
				"$hud_size": [ "90%", "100%" ]
			}
		],
		"size": "$hud_size",
		"controls": [
			{ "camera_renderer@camera_renderer": {} },
			{ "root_panel@hud.root_panel": {} },
			{
				"editor_gizmo_renderer@editor_gizmo_renderer": {
					"ignored": "(not $is_editor_mode_enabled)"
				}
			},
			{ "bottom_hud@bottom_hud": { "offset": [ 0, -12 ] } }
		]
	},
    
	"hud_screen@common.base_screen": {
		"enable_profanity_filter": false,
		"is_showing_menu": false,
		"should_steal_mouse": true,
		"low_frequency_rendering": false,
		"absorbs_input": false,
		"send_telemetry": false
	}
}
