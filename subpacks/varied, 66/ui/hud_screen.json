{
  "namespace": "hud",
  
	"chat_text": {
		"type": "label",
		"layer": 10,
		"text": "#text",
		"size": [ 268, "default" ],
		"shadow": true,
		"enable_profanity_filter": false,
		"localize": true,
		"font_scale_factor": 0.6666,
		"color": "$chat_text_color",
		"anims": [
		"@iihud.ii_slide_anim",
		"@iihud.ii_fade_anim"
		]
	},
    
	"hud_screen@common.base_screen": {
		"$screen_content": "iihud.content",
		"enable_profanity_filter": false,
		"is_showing_menu": false,
		"should_steal_mouse": true,
		"load_screen_immediately": true,
		"low_frequency_rendering": false,
		"absorbs_input": false,
		"send_telemetry": false
	}
}
