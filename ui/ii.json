		]
	},
	
	"version_label": {
		"type": "label",
		"alpha": 0.5,
		"text": "#version",
		"size": [ "default", 10 ],
		"anchor_from": "bottom_right",
		"anchor_to": "bottom_right",
		"offset": [ -1, 4 ],
		"font_scale_factor": 0.5,
		"bindings": [ { "binding_name": "#version" } ]
	},
	
	"left_invisible_button@ii.button_asset": {
		"layer": 22,
		"size": [ 32, "100%" ],
		"anchor_from": "top_left",
		"anchor_to": "top_left",
		"offset": [ 0, 0 ],
		"$hover_state": false,
		"$locked_alpha": 0.1,
		"controls": [
			{ "hover@ii.left_invisible_button_hover": {} }
