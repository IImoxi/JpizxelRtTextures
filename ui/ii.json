	"player_inventory": {
		"type": "panel",
		"size": [ 0, 0 ],
		"controls": [
			{
				"inventory_grid": {
					"type": "grid",
					"size": [ 162, 54 ],
					"grid_dimensions": [ 9, 3 ],
